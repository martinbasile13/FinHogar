// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ingresos {
  id Int @id @default(autoincrement())
  nombre String
  tipo String
  procedencia String
  frecuencia String
  monto Int 
}